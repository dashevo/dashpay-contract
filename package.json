{
  "name": "@dashevo/dashpay-contract",
  "version": "0.1.0",
  "description": "Reference contract of the DashPay DPA on Dash Evolution",
  "scripts": {
    "check-package": "npm run check-package:name && npm run check-package:version",
    "check-package:name": "test $(jq -r .name package.json) = $(jq -r .name package-lock.json)",
    "check-package:version": "test $(jq -r .version package.json) = $(jq -r .version package-lock.json)",
    "lint": "eslint .",
    "test": "npm run test:unit",
    "test:unit": "mocha 'test/unit/*.spec.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dashevo/dashpay-contract.git"
  },
  "author": "Dash Core Team",
  "contributors": [
    {
      "name": "Alex Werner",
      "email": "alex@dash.org",
      "url": "https://github.com/alex-werner"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dashevo/dashpay-contract/issues"
  },
  "homepage": "https://github.com/dashevo/dashpay-contract#readme",
  "devDependencies": {
    "@dashevo/dpp": "^0.17.0",
    "chai": "^4.2.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "mocha": "^8.1.1",
    "sinon": "^9.0.3",
    "sinon-chai": "^3.5.0"
  }
}
